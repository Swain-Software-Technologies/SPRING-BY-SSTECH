<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!--  byName Explanation -->
	<bean id="tourPlan"
		class="com.sstech.spring.core.aw.beans.TourPlan">
		<property name="places">
			<array>
				<value>Otty</value>
				<value>Kudaikenal</value>
				<value>MunnarHills</value>
			</array>
		</property>
	</bean>
	<bean id="tourPlan1"
		class="com.sstech.spring.core.aw.beans.TourPlan">
		<property name="places">
			<array>
				<value>Konark</value>
				<value>Puri</value>
				<value>National Zoo</value>
			</array>
		</property>
	</bean>
	<bean id="travelAgent" class="com.sstech.spring.core.aw.beans.TravelAgent" autowire="byName"/> 
	
	
</beans>
